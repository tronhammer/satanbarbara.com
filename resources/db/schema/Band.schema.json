{
	"name": "Band",
	"listingKey": "bands",
	"domain": "satanbarbara",
	"version": "1",
	"meta": {
		"hookes": {
			"php": {
				"format": "mvc"
			}
		},
		"db": {
			"mysql": {
				"5.5": {
					"populate": 1,
					"id": "id",
					"junction": {
						"bandCreator": "Account",
						"bandMember": "Account",
						"bandGenre": "Descriptor"
					}
				}
			}
		}
	},
	"properties": {
		"id": {
			"type": "id",
			"unique": true,
			"name": "id",
			"label": "Unique ID",
			"description": "",
			"generator": "db",
			"incremented": true
		},
		"created": {
			"type": "timestamp",
			"name": "created",
			"label": "Created",
			"description": "Time when entry was made.",
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "db"
		},
		"name": {
			"type": "string",
			"max": 255,
			"min": 3,
			"name": "name",
			"label": "Name",
			"description": "",
			"default": "",
			"placeholder": "Chao Lux",
			"generator": "user",
			"required": true
		}, 
		"description": {
			"type": "string",
			"max": 4000,
			"name": "description",
			"label": "Description",
			"description": "",
			"default": "",
			"placeholder": "The best..fffucken...metAL Band OUT!! there...",
			"generator": "user"
		},
		"origin": {
			"type": "string",
			"max": 255,
			"min": 3,
			"name": "origin",
			"label": "Origin",
			"description": "",
			"default": "",
			"placeholder": "Satans asshole",
			"generator": "user"
		},
		"active": {
			"type": "option",
			"name": "active",
			"label": "Active",
			"description": "",
			"default": "yes",
			"generator": "user",
			"options": {
				"yes": {
					"type": "string",
					"name": "yes",
					"label": "Yes",
					"description": "",
					"position": 0
				},
				"no": {
					"type": "string",
					"name": "no",
					"label": "No",
					"description": "",
					"position": 1
				}
			}
		},
		"last_login": {
			"type": "timestamp",
			"name": "last_login",
			"label": "Last logged in",
			"description": "",
			"default": 0,
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "system"
		},
		"last_active": {
			"type": "timestamp",
			"name": "last_active",
			"label": "Last active",
			"description": "",
			"default": 0,
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "system"
		},
		"last_activated": {
			"type": "timestamp",
			"name": "last_activated",
			"label": "Activated",
			"description": "Last time the band was reactivated after having been marked as suspended or disbanded/hiatus",
			"default": 0,
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "system",
			"restricted": true
		},
		"last_modified": {
			"type": "timestamp",
			"name": "last_modified",
			"label": "Last modified",
			"description": "",
			"default": "",
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "system",
			"restricted": true
		},
		"activated": {
			"type": "boolean",
			"name": "activated",
			"label": "Activated",
			"description": "",
			"default": 0,
			"placeholder": "1",
			"generator": "system",
			"restricted": true
		},
		"suspended": {
			"type": "boolean",
			"name": "suspended",
			"label": "Suspended",
			"description": "",
			"default": 0,
			"placeholder": "0",
			"generator": "system",
			"restricted": true
		},
		"banned": {
			"type": "boolean",
			"name": "banned",
			"label": "Banned",
			"description": "",
			"default": 0,
			"placeholder": "0",
			"generator": "system",
			"restricted": true
		},
		"archived": {
			"type": "boolean",
			"name": "archived",
			"label": "Archived",
			"description": "",
			"default": 0,
			"placeholder": "",
			"generator": "system",
			"restricted": true
		},
		"deleted": {
			"type": "boolean",
			"name": "deleted",
			"label": "Deleted",
			"description": "",
			"default": 0,
			"placeholder": "0",
			"generator": "system",
			"restricted": true
		}
	}
}