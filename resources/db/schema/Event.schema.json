{
	"name": "Event",
	"listingKey": "events",
	"domain": "satanbarbara",
	"version": "1",
	"meta": {
		"hookes": {
			"php": {
				"format": "mvc"
			}
		},
		"db": {
			"mysql": {
				"5.5": {
					"populate": 1,
					"id": "id",
					"junction": {
						"eventCreator": "Account",
						"eventAttendee": "Account",
						"eventVenue": "Venue",
						"eventBand": "Band",
						"eventGenre": "Descriptor"
					}
				}
			}
		}
	},
	"properties": {
		"id": {
			"type": "id",
			"unique": true,
			"name": "id",
			"label": "Unique ID",
			"description": "",
			"generator": "db",
			"required": true,
			"incremented": true
		},
		"created": {
			"type": "timestamp",
			"name": "created",
			"label": "Created",
			"description": "Time when entry was made.",
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "db"
		},
		"title": {
			"type": "string",
			"max": 255,
			"min": 3,
			"name": "title",
			"label": "Title",
			"description": "",
			"default": "",
			"placeholder": "SBCMT Bible Burning",
			"generator": "user",
			"required": true
		}, 
		"subtitle": {
			"type": "string",
			"max": 90,
			"name": "subtitle",
			"label": "Subtitle",
			"description": "",
			"default": "",
			"placeholder": "B-Y-O-Bible",
			"generator": "user"
		}, 
		"description": {
			"type": "string",
			"max": 4000,
			"name": "description",
			"label": "Description",
			"description": "",
			"default": "",
			"placeholder": "This is gonna be hot!",
			"generator": "user"
		},
		"date_text": {
			"type": "string",
			"max": 35,
			"name": "date_text",
			"label": "Date Full Text",
			"description": "2014-11-25 04:34:00 pm",
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "system"
		},
		"date": {
			"type": "date",
			"name": "date",
			"label": "Event Date",
			"default": "",
			"placeholder": "2014-12-27",
			"description": "",
			"generator": "user",
			"required": true
		},
		"start_time": {
			"type": "timestamp",
			"name": "start_time",
			"label": "Start Time",
			"description": "",
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "user",
			"required": true
		},
		"end_time": {
			"type": "timestamp",
			"name": "end_time",
			"label": "End Time",
			"description": "",
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "user",
			"required": true
		},
		"ages": {
			"type": "option",
			"name": "ages",
			"label": "Ages",
			"description": "",
			"generator": "user",
			"default": "na",
			"options": {
				"all": {
					"type": "string",
					"name": "all",
					"label": "All",
					"description": "",
					"position": 0
				},
				"18": {
					"type": "string",
					"name": "18",
					"label": "18+",
					"description": "",
					"position": 1
				},
				"21": {
					"type": "string",
					"name": "21",
					"label": "21+",
					"description": "",
					"position": 2
				},
				"na": {
					"type": "string",
					"name": "na",
					"label": "Not Sure",
					"description": "",
					"position": 35
				}
			}
		},
		"price": {
			"type": "string",
			"name": "price",
			"label": "Event Price",
			"description": "",
			"default": "",
			"placeholder": "$5",
			"generator": "user",
			"required": true
		},
		"requirements": {
			"type": "string",
			"max": 4000,
			"name": "requirements",
			"label": "Requirements",
			"description": "",
			"placeholder": "At least 1 bible.",
			"default": "",
			"generator": "user"
		},
		"ticket_uri": {
			"type": "string",
			"max": 255,
			"name": "ticket_uri",
			"label": "Ticket URL",
			"description": "",
			"default": "",
			"placeholder": "http://",
			"generator": "user"
		},
		"flyer_uri": {
			"type": "string",
			"max": 255,
			"name": "flyer_uri",
			"label": "Flyer",
			"description": "",
			"default": "",
			"placeholder": "http://",
			"generator": "user"
		},
		"promocode": {
			"type": "string",
			"max": 64,
			"name": "promocode",
			"label": "Promocode",
			"placeholder": "IKICKASSFORTHELORD",
			"description": "",
			"generator": "user"
		},
		"last_modified": {
			"type": "timestamp",
			"name": "last_modified",
			"label": "Last modified",
			"description": "",
			"default": "",
			"placeholder": "2014-11-25 04:34:00 pm",
			"generator": "system"
		},
		"canceled": {
			"type": "boolean",
			"name": "canceled",
			"label": "Canceled",
			"description": "",
			"default": 0,
			"placeholder": "0",
			"generator": "system"
		},
		"archived": {
			"type": "boolean",
			"name": "archived",
			"label": "Archived",
			"description": "",
			"default": 0,
			"placeholder": "0",
			"generator": "system"
		},
		"deleted": {
			"type": "boolean",
			"name": "deleted",
			"label": "Deleted",
			"description": "",
			"default": 0,
			"placeholder": "0",
			"generator": "system"
		}
	}
}